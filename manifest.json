{
 
  "manifest_version": 2,
  "name": "OrnaJS",
  "version": "3.0.0",
  "description": "Orna - Atomic CSS Parser. Good tool for web developer!",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "128": "icon128.png" },
  "author": "Dima Popov",
  "content_scripts": [
    {
      "matches": ["*://orna/*", "file:///*/orna*"],
      "js": ["jquery.js","background.js"],
      "run_at": "document_end"

    }
  ],
  "homepage_url": "http://ornaorg.github.io/",
     "web_accessible_resources": [
    
         "/jquery.js",
         "/orna.js"
  ]
 
}